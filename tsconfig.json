{
	"compileOnSave": false,
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2016",
	},
	"exclude": [
	  "node_modules"
  ]
}
